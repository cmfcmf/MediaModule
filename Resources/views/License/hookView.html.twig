{% if licenses is not empty %}
    <h3>{% trans %}Licenses{% endtrans %}</h3>
    {% trans %}This content is licensed under the following licenses{% endtrans %}:
    <ul>
        {% for license in licenses %}
            <li>
                {% if license.imageUrl is not empty %}
                    <img src="{{ license.imageUrl|e('html_attr') }}" alt="" />
                {% endif %}
                {% if license.url is not empty %}
                    <a href="{{ license.url|e('html_attr') }}">
                {% endif %}
                {{ license.title }}
                {% if license.url is not empty %}
                    </a>
                {% endif %}
            </li>
        {% endfor %}
    </ul>
{% endif %}
