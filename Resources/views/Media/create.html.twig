{% extends 'CmfcmfMediaModule::base.html.twig' %}
{% block title __('Create new media') %}
{% block content %}
    {% include 'CmfcmfMediaModule::util.html.twig' %}
    {% form_theme form 'CmfcmfMediaModule:Form:bootstrap_3.html.twig' %}

    {{ form_start(form) }}
    {{ form_widget(form) }}

    {{ hook|raw }}

    <div class="col-sm-offset-2 col-sm-10">
        <button class="btn btn-success" type="submit">
            {{ __('Save') }}
        </button>
    </div>
    {{ form_end(form) }}
{% endblock %}
